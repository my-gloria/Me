//Implement 2D array input and output in C
#include <stdio.h>

int main() {
    int arr[2][3];
    int i, j;

    printf("Enter elements:\n");

    for(i = 0; i < 2; i++) {
        for(j = 0; j < 3; j++) {
            scanf("%d", &arr[i][j]);
        }
    }

    printf("Matrix is:\n");

    for(i = 0; i < 2; i++) {
        for(j = 0; j < 3; j++) {
            printf("%d ", arr[i][j]);
        }
        printf("\n");
    }

    return 0;
}


//Implement 2D array and basic calculator in C


#include <stdio.h>

int main() {
    int num1, num2, op;
    float result;

    printf("Enter two numbers: ");
    scanf("%d %d", &num1, &num2);

    printf("Choose operation: 1=+  2=-  3=*  4=/ : ");
    scanf("%d", &op);

    if(op == 1) result = num1 + num2;
    else if(op == 2) result = num1 - num2;
    else if(op == 3) result = num1 * num2;
    else if(op == 4) {
        if(num2 == 0) { printf("Cannot divide by 0\n"); return 0; }
        result = (float)num1 / num2;
    }
    else { printf("Invalid choice\n"); return 0; }

    printf("Result = %.2f\n", result);
    return 0;
}


//Implement recursive number printing function

#include <stdio.h>

void printNumbers(int n) {
    if(n > 50) return;     
    printf("%d ", n);
    printNumbers(n + 1);   
}

int main() {
    printNumbers(1);
    return 0;
}


//

#include <stdio.h>

int fibonacci(int n) {
    if(n == 0) return 0;
    if(n == 1) return 1;
    return fibonacci(n - 1) + fibonacci(n - 2);
}

int main() {
    int i;
    printf("Fibonacci series: ");
    for(i = 0; i < 10; i++) {  
        printf("%d ", fibonacci(i));
    }
    return 0;
}
